# RSSHub settings
DEBUG_INFO=false
DISALLOW_ROBOT=true
# RSSHub Cache settings
CACHE_TYPE=memory
CACHE_CONTENT_EXPIRE=3600
RSSHUB_REFRESH_SECONDS=300
# RSSHub Twitter(X) Credentials
TWITTER_USERNAME=
TWITTER_PASSWORD=
TWITTER_AUTHENTICATION_SECRET=
TWITTER_PHONE_OR_EMAIL=
TWITTER_AUTH_TOKEN=
TWITTER_THIRD_PARTY_API=

# Discord Bot Token
DISCORD_BOT_TOKEN=BotYourTokenHere
# Discord Guild IDs (comma-separated)
GUILD_IDS=
# Minimum polling interval (seconds)
DEFAULT_POLL_INTERVAL_SECONDS=60
MIN_POLL_INTERVAL_SECONDS=60
# Subscription settings file (relative to repo root)
SUBSCRIPTIONS_PATH=subscriptions.json
