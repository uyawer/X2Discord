# RSSHub settings
DEBUG_INFO=false
DISALLOW_ROBOT=true
# RSSHub Cache settings
CACHE_TYPE=memory
CACHE_CONTENT_EXPIRE=3600
RSSHUB_REFRESH_SECONDS=300
# RSSHub Twitter(X) Credentials
TWITTER_USERNAME=
TWITTER_PASSWORD=
TWITTER_AUTHENTICATION_SECRET=
TWITTER_PHONE_OR_EMAIL=
TWITTER_AUTH_TOKEN=
TWITTER_THIRD_PARTY_API=

# Discord Bot Token (required)
DISCORD_BOT_TOKEN=BotYourTokenHere

# Discord Guild IDs (comma-separated, optional)
GUILD_IDS=

# RSSHub Base URL
# For docker compose: http://rsshub:1200
# For local development: http://localhost:1200
RSSHUB_BASE_URL=http://rsshub:1200

# Redis URL for persisting sent links (required)
# For docker compose: redis://redis:6379/0
# For local development: redis://localhost:6379/0
REDIS_URL=redis://redis:6379/0

# Polling interval settings (seconds)
DEFAULT_POLL_INTERVAL_SECONDS=60
MIN_POLL_INTERVAL_SECONDS=60

# Subscription settings file path
SUBSCRIPTIONS_PATH=subscriptions.json
